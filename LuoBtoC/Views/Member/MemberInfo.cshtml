@{
    ViewBag.Title = "会员基本资料";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model SqlModel.MemberModel.Member
<div id="content" class="float_r">
    <h5 style="text-align:left">会员中心 -》我的基本信息</h5>
    <div class="content_half float_l checkout">
        会员名:
        <p>@Model.MemberName</p>
        <br />
        <br />
        会员等级:
        <p>@Model.MemberRankId</p>
        <br />
        <br />
        邮箱:
        <p>@Model.Email</p>
        <br />
        <br />
        电话:
        <p>@Model.Phone</p>
        <br />
        <br />
        QQ号:
        <p>@Model.QQ</p>
        <br />
        <br />
        地址:
        <p>@Model.Address</p>
        <br />
        <br />
        <a href="javascript:location.href='MemberInfoUpdate.aspx?id=@Model.ID'">
        <img src="../images/btn_Update.jpg"  alt="" height="25px" width="75px" /></a>
    </div>
</div>
